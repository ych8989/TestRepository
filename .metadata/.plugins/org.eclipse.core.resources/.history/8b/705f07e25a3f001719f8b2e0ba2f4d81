package com.mycompany.myapp.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.mycompany.myapp.dao.Exam10Dao2;

@Component
public class Exam10Service2Impl implements Exam10Service2 {
	private Exam10Dao2 exam10Dao2;
	// Setter 주입

	@Autowired
	public void setExam10Dao2(Exam10Dao2 exam10Dao2) {
		this.exam10Dao2 = exam10Dao2;
		//추가적인 초기화 코드

	}

	public void join() {
		System.out.println("Exam10Service2Impl-join() 실행");
		exam10Dao2.insert();

	}

	public void login() {
		System.out.println("Exam10Service2Impl-login() 실행");
		exam10Dao2.select();

	}

}
