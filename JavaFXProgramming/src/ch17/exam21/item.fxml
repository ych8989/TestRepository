<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView id="image" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@images/food01.png" />
            </image>
        </ImageView>
        <VBox maxWidth="1.7976931348623157E308" spacing="10.0" style="-fx-background-color: #C4C4C4;" HBox.hgrow="ALWAYS">
            <children>
                <Label id="name" text="음식이름" />
                <ImageView id="score" fitHeight="20.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@images/star1.png" />
                    </image>
                </ImageView>
                <Label id="description" text="설명내용" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
    </children>
</HBox>
